{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start:mac": "EXPO_PUBLIC_API_BASE=http://$(ipconfig getifaddr en0):8080 EXPO_PUBLIC_DEV_MODE=true expo start",
    "start:linux": "EXPO_PUBLIC_API_BASE=http://$(hostname -I | awk '{print $1}'):8080 EXPO_PUBLIC_DEV_MODE=true expo start",
    "start:win": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"$ip=(Get-NetIPAddress -AddressFamily IPv4 | ?{ $_.IPAddress -notlike '169.*' -and $_.InterfaceAlias -notmatch 'vEthernet|Loopback' } | select -First 1 -Expand IPAddress); $env:EXPO_PUBLIC_API_BASE=\\\"http://$ip:8080\\\"; $env:EXPO_PUBLIC_DEV_MODE=\\\"true\\\"; npx expo start\"",
    "start:prod": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@react-navigation/stack": "^7.4.8",
    "expo": "~53.0.22",
    "expo-asset": "~11.1.7",
    "expo-auth-session": "~6.0.2",
    "expo-crypto": "~14.0.1",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "~14.0.1",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "react-native-outside-press": "^1.2.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true
}
