{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start:mac": "EXPO_PUBLIC_API_BASE=http://$(ipconfig getifaddr en0):8080 expo start",
    "start:linux": "EXPO_PUBLIC_API_BASE=http://$(hostname -I | awk '{print $1}'):8080 expo start",
    "start:win": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"$ip=(Get-NetIPAddress -AddressFamily IPv4 | ?{ $_.IPAddress -notlike '169.*' -and $_.InterfaceAlias -notmatch 'vEthernet|Loopback' } | select -First 1 -Expand IPAddress); $env:EXPO_PUBLIC_API_BASE=\\\"http://$ip:8080\\\"; npx expo start\"",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "expo": "~53.0.22",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "^0.79.5",
    "react-native-safe-area-context": "^5.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true
}